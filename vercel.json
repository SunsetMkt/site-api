{
    "functions": {
        "app.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api",
            "destination": "/app.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/app.py"
        }
    ]
}