{
    "functions": {
        "api/app.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/",
            "destination": "/api/app.py"
        },
        {
            "source": "/api/:match*",
            "destination": "/api/app.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/404.js"
        }
    ]
}